/delete-node/ &storage_partition;

&flash0 {
	partitions {
		compatible = "fixed-partitions";
		#address-cells = <1>;
		#size-cells = <1>;

        storage_partition: partition@3b0000 {
			label = "storage";
			reg = <0x3B0000 DT_SIZE_K(128)>;
		};

		astarte_partition: partition@3d0000 {
			label = "astarte";
			reg = <0x003D0000 DT_SIZE_K(64)>;
		};
	};
};
